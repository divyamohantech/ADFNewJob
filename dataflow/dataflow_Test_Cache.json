{
	"name": "dataflow_Test_Cache",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "SQLDynamicDS",
						"type": "DatasetReference"
					},
					"name": "SourceColorTool"
				}
			],
			"sinks": [
				{
					"name": "Cache"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          COLOR_CHIP_ID as integer,",
				"          PANEL_ID as integer,",
				"          CHIP_SOURCE as string,",
				"          IS_ACTIVE as integer,",
				"          LAST_UPDATED_DATE as timestamp,",
				"          COLOR_TOOL_ID as integer,",
				"          SOURCE_SYSTEM_NAME as string,",
				"          SOURCE_SYSTEM_ID as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'Select  max(COLOR_TOOL_ID) as Max_COLOR_TOOL_ID from [dbo].[COLOR_TOOL]',",
				"     format: 'query') ~> SourceColorTool",
				"SourceColorTool sink(validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     store: 'cache',",
				"     format: 'inline',",
				"     output: true,",
				"     saveOrder: 1) ~> Cache"
			]
		}
	}
}