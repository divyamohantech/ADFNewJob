{
	"name": "dataflow_dynamic_2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "SQLDynamicDS",
						"type": "DatasetReference"
					},
					"name": "SourceInCsv"
				},
				{
					"dataset": {
						"referenceName": "SQLDynamicDS",
						"type": "DatasetReference"
					},
					"name": "SourceUserProfile"
				},
				{
					"dataset": {
						"referenceName": "SQLDynamicDS",
						"type": "DatasetReference"
					},
					"name": "SourcePanel"
				},
				{
					"dataset": {
						"referenceName": "SQLDynamicDS",
						"type": "DatasetReference"
					},
					"name": "INCSV"
				}
			],
			"sinks": [
				{
					"name": "SinkEntityIDValue"
				},
				{
					"dataset": {
						"referenceName": "SQLDynamicDS",
						"type": "DatasetReference"
					},
					"name": "sinkPanelTable"
				},
				{
					"dataset": {
						"referenceName": "DelimitedTextDymanicCSVFiles",
						"type": "DatasetReference"
					},
					"name": "SinkRejectionCSV"
				}
			],
			"transformations": [
				{
					"name": "AddColumnsValueEntityID"
				},
				{
					"name": "select1"
				},
				{
					"name": "derivedColumn1",
					"description": "Creating/updating the columns 'COLOR_Tool_Name, ENTITY_ID'"
				},
				{
					"name": "CommanJoin"
				},
				{
					"name": "selectColumns"
				},
				{
					"name": "NotExistsUnique"
				}
			],
			"scriptLines": [
				"source(output(",
				"          COLOR_CHIP_ID as integer,",
				"          PANEL_ID as integer,",
				"          CHIP_SOURCE as string,",
				"          IS_ACTIVE as integer,",
				"          LAST_UPDATED_DATE as timestamp,",
				"          COLOR_TOOL_ID as integer,",
				"          SOURCE_SYSTEM_NAME as string,",
				"          SOURCE_SYSTEM_ID as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> SourceInCsv",
				"source(output(",
				"          ENTITY_ID as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'Select ENTITY_ID as ENTITY_ID from [dbo].[USER_PROFILE] Where User_Name =\\'pfadmin\\'',",
				"     format: 'query') ~> SourceUserProfile",
				"source(output(",
				"          COLOR_CHIP_ID as integer,",
				"          PANEL_ID as integer,",
				"          CHIP_SOURCE as string,",
				"          IS_ACTIVE as integer,",
				"          LAST_UPDATED_DATE as timestamp,",
				"          COLOR_TOOL_ID as integer,",
				"          SOURCE_SYSTEM_NAME as string,",
				"          SOURCE_SYSTEM_ID as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> SourcePanel",
				"source(output(",
				"          COLOR_CHIP_ID as integer,",
				"          PANEL_ID as integer,",
				"          CHIP_SOURCE as string,",
				"          IS_ACTIVE as integer,",
				"          LAST_UPDATED_DATE as timestamp,",
				"          COLOR_TOOL_ID as integer,",
				"          SOURCE_SYSTEM_NAME as string,",
				"          SOURCE_SYSTEM_ID as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> INCSV",
				"SourceUserProfile derive(ENTITY_ID = toInteger(ENTITY_ID)) ~> AddColumnsValueEntityID",
				"SourceInCsv select(mapColumn(",
				"          COLOR_Tool_Name",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 derive(ENTITY_ID = SinkEntityIDValue#output()) ~> derivedColumn1",
				"INCSV, SourcePanel join(Color_Chip_Name == NAME,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> CommanJoin",
				"CommanJoin select(mapColumn(",
				"          NAME,",
				"          ENTITY_ID",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectColumns",
				"derivedColumn1, SourcePanel exists(COLOR_Tool_Name == NAME,",
				"     negate:true,",
				"     broadcast: 'auto')~> NotExistsUnique",
				"AddColumnsValueEntityID sink(validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     store: 'cache',",
				"     format: 'inline',",
				"     output: true,",
				"     saveOrder: 2,",
				"     mapColumn(",
				"          ENTITY_ID",
				"     )) ~> SinkEntityIDValue",
				"NotExistsUnique sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          COLOR_CHIP_ID as integer,",
				"          PANEL_ID as integer,",
				"          CHIP_SOURCE as string,",
				"          IS_ACTIVE as integer,",
				"          LAST_UPDATED_DATE as timestamp,",
				"          COLOR_TOOL_ID as integer,",
				"          SOURCE_SYSTEM_NAME as string,",
				"          SOURCE_SYSTEM_ID as integer",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     saveOrder: 3,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          NAME = COLOR_Tool_Name,",
				"          ENTITY_ID = ENTITY_ID.ENTITY_ID",
				"     )) ~> sinkPanelTable",
				"selectColumns sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          COLOR_TOOL_ID as string,",
				"          COLOR_TOOL_NAME as string,",
				"          COLOR_TOOL_TYPE_ID as string",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     saveOrder: 1) ~> SinkRejectionCSV"
			]
		}
	}
}