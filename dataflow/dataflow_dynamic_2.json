{
	"name": "dataflow_dynamic_2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "SQLDynamicDS",
						"type": "DatasetReference"
					},
					"name": "SourceInCsv"
				},
				{
					"dataset": {
						"referenceName": "SQLDynamicDS",
						"type": "DatasetReference"
					},
					"name": "SourceUserProfile"
				},
				{
					"dataset": {
						"referenceName": "SQLDynamicDS",
						"type": "DatasetReference"
					},
					"name": "SourcePanel"
				}
			],
			"sinks": [
				{
					"name": "SinkEntityIDValue"
				},
				{
					"dataset": {
						"referenceName": "SQLDynamicDS",
						"type": "DatasetReference"
					},
					"name": "SinkPanelTable",
					"description": "Add sink dataset panel table"
				}
			],
			"transformations": [
				{
					"name": "AddColumnsValueEntityID"
				},
				{
					"name": "select1"
				},
				{
					"name": "derivedColumn1",
					"description": "Creating/updating the columns 'COLOR_Tool_Name, ENTITY_ID'"
				},
				{
					"name": "ExistsTestCondition"
				}
			],
			"scriptLines": [
				"source(output(",
				"          COLOR_TOOL_ID as integer,",
				"          COLOR_TOOL_NAME as string,",
				"          COLOR_TOOL_TYPE_ID as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> SourceInCsv",
				"source(output(",
				"          PANEL_ID as integer,",
				"          NAME as string,",
				"          IS_METALLIC as integer,",
				"          CREATE_USER_ID as integer,",
				"          CREATE_DATE as timestamp,",
				"          NOTES as string,",
				"          IS_PUBLIC as integer,",
				"          ENTITY_ID as integer,",
				"          IS_INACTIVE as integer,",
				"          AUDIT_SEARCH_INDEX_DATE as timestamp,",
				"          IS_FLAGGED as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'Select ENTITY_ID as ENTITY_ID from [dbo].[USER_PROFILE] Where User_Name =\\'pfadmin\\'',",
				"     format: 'query') ~> SourceUserProfile",
				"source(output(",
				"          COLOR_TOOL_ID as integer,",
				"          COLOR_TOOL_NAME as string,",
				"          COLOR_TOOL_TYPE_ID as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> SourcePanel",
				"SourceUserProfile derive(ENTITY_ID = toInteger(ENTITY_ID)) ~> AddColumnsValueEntityID",
				"SourceInCsv select(mapColumn(",
				"          COLOR_Tool_Name",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 derive(ENTITY_ID = SinkEntityIDValue#output()) ~> derivedColumn1",
				"derivedColumn1, SourcePanel exists(COLOR_Tool_Name == NAME,",
				"     negate:true,",
				"     broadcast: 'auto')~> ExistsTestCondition",
				"AddColumnsValueEntityID sink(validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     store: 'cache',",
				"     format: 'inline',",
				"     output: true,",
				"     saveOrder: 0,",
				"     mapColumn(",
				"          ENTITY_ID",
				"     )) ~> SinkEntityIDValue",
				"ExistsTestCondition sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          COLOR_TOOL_ID as integer,",
				"          COLOR_TOOL_NAME as string,",
				"          COLOR_TOOL_TYPE_ID as integer",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          NAME = COLOR_Tool_Name,",
				"          ENTITY_ID = ENTITY_ID.ENTITY_ID",
				"     )) ~> SinkPanelTable"
			]
		}
	}
}